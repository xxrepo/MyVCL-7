<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <MainSource>SpellCheckerSimpleDemo.cpp</MainSource>
    <Config Condition="'$(Config)'==''">Base</Config>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Config)'=='Base' or '$(Base)'!=''">
    <Base>true</Base>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Base)'!=''">
    <DCC_CBuilderOutput>JPHNE</DCC_CBuilderOutput>
    <Defines>NO_STRICT</Defines>
    <ProjectType>CppVCLApplication</ProjectType>
    <BCC_wpar>false</BCC_wpar>
    <IncludePath>$(BDS)\include;$(BDS)\include\dinkumware;$(BDS)\include\vcl;..\Common;..</IncludePath>
    <AllPackageLibs>rtl.lib;vcl.lib;dxSpellCheckerC11.lib;cxEditorsC11.lib;cxExtEditorsC11.lib;cxLibraryC11.lib;dxGDIPlusC11.lib;cxDataC11.lib;dxThemeC11.lib;vcldb.lib;dbrtl.lib;bdertl.lib;adortl.lib;vclx.lib;;dxCoreC11.lib</AllPackageLibs>
    <ILINK_LibraryPath>$(BDS)\lib\release;$(BDS)\lib;$(BDS)\lib\obj;$(BDS)\lib\psdk;..\Common;..</ILINK_LibraryPath>
  </PropertyGroup>
  <ProjectExtensions>
    <Borland.Personality>CPlusPlusBuilder.Personality</Borland.Personality>
    <Borland.ProjectType>CppVCLApplication</Borland.ProjectType>
    <BorlandProject>
      <BorlandProject>
        <CPlusPlusBuilder.Personality>
        </CPlusPlusBuilder.Personality>
      </BorlandProject>
    </BorlandProject>
  </ProjectExtensions>
  <Import Project="$(MSBuildBinPath)\Borland.Cpp.Targets" />
  <ItemGroup>
    <ResFiles Include="SpellCheckerSimpleDemo.res">
    </ResFiles>
    <CppCompile Include="SpellCheckerSimpleDemo.cpp"> </CppCompile>
    <CppCompile Include="SimpleDemoMain.cpp">
      <Form>fmCV</Form>
    </CppCompile>
    <CppCompile Include="..\Common\DemoUtils.cpp"> </CppCompile>
    <FormResources Include="SimpleDemoMain.dfm" />
  </ItemGroup>
</Project>