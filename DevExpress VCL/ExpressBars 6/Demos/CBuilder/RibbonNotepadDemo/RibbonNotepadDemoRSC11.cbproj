<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <MainSource>RibbonNotepadDemo.cpp</MainSource>
    <Config Condition="'$(Config)'==''">Base</Config>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Config)'=='Base' or '$(Base)'!=''">
    <Base>true</Base>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Base)'!=''">
    <DCC_CBuilderOutput>JPHNE</DCC_CBuilderOutput>
    <Defines>NO_STRICT</Defines>
    <ProjectType>CppVCLApplication</ProjectType>
    <BCC_wpar>false</BCC_wpar>
    <IncludePath>$(BDS)\include;$(BDS)\include\dinkumware;$(BDS)\include\vcl;..\Common;..</IncludePath>
    <AllPackageLibs>rtl.lib;vcl.lib;vcldb.lib;dbrtl.lib;bdertl.lib;adortl.lib;VCLX.lib;dxComnRS11.lib;dxThemeRS11.lib;cxLibraryRS11.lib;dxBarRS11.lib;dxBarExtItemsRS11.lib;dxBarExtDBItemsRS11.lib;dxBarDBNavRS11.lib;dxSBRS11.lib;dxRibbonRS11.lib;dxGDIPlusRS11.lib;dxCoreRS11.lib</AllPackageLibs>
    <ILINK_LibraryPath>$(BDS)\lib\release;$(BDS)\lib;$(BDS)\lib\obj;$(BDS)\lib\psdk;..\Common;..</ILINK_LibraryPath>
  </PropertyGroup>
  <ProjectExtensions>
    <Borland.Personality>CPlusPlusBuilder.Personality</Borland.Personality>
    <Borland.ProjectType>CppVCLApplication</Borland.ProjectType>
    <BorlandProject>
      <BorlandProject>
        <CPlusPlusBuilder.Personality>
        </CPlusPlusBuilder.Personality>
      </BorlandProject>
    </BorlandProject>
  </ProjectExtensions>
  <Import Project="$(MSBuildBinPath)\Borland.Cpp.Targets" />
  <ItemGroup>
    <ResFiles Include="RibbonNotepadDemo.res">
    </ResFiles>
    <CppCompile Include="RibbonNotepadDemo.cpp"> </CppCompile>
    <CppCompile Include="RibbonDemoAbout.cpp">
      <Form>RibbonDemoAboutForm</Form>
    </CppCompile>
    <CppCompile Include="RibbonNotepadDemoMain.cpp">
      <Form>RibbonDemoMainForm</Form>
    </CppCompile>
    <CppCompile Include="RibbonNotepadDemoOptions.cpp">
      <Form>RibbonDemoOptionsForm</Form>
    </CppCompile>
    <CppCompile Include="RibbonNotepadDemoGallerySetup.cpp">
      <Form>ColorDialogSetupForm</Form>
    </CppCompile>
    <CppCompile Include="..\Common\EBarsDemoRating.cpp">
      <Form>EBarsDemoRatingForm</Form>
    </CppCompile>
    <CppCompile Include="..\Common\EBarsAbout.cpp">
      <Form>EBarsAboutForm</Form>
    </CppCompile>
    <CppCompile Include="..\Common\EBarsUtils.cpp">
      <Form>dmCommonData</Form>
    </CppCompile>
    <FormResources Include="RibbonDemoAbout.dfm" />
    <FormResources Include="RibbonNotepadDemoMain.dfm" />
    <FormResources Include="RibbonNotepadDemoOptions.dfm" />
    <FormResources Include="RibbonNotepadDemoGallerySetup.dfm" />
    <FormResources Include="..\Common\EBarsDemoRating.dfm" />
    <FormResources Include="..\Common\EBarsAbout.dfm" />
    <FormResources Include="..\Common\EBarsUtils.dfm" />
  </ItemGroup>
</Project>