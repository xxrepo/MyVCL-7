<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <MainSource>PuzzleDemo.cpp</MainSource>
    <Config Condition="'$(Config)'==''">Base</Config>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Config)'=='Base' or '$(Base)'!=''">
    <Base>true</Base>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Base)'!=''">
    <DCC_CBuilderOutput>JPHNE</DCC_CBuilderOutput>
    <Defines>NO_STRICT</Defines>
    <ProjectType>CppVCLApplication</ProjectType>
    <BCC_wpar>false</BCC_wpar>
    <IncludePath>$(BDS)\include;$(BDS)\include\dinkumware;$(BDS)\include\vcl;..\Common;..</IncludePath>
    <AllPackageLibs>rtl.lib;vcl.lib;rtl.lib;;cxLibraryC11.lib;dxThemeC11.lib;dxGDIPlusC11.lib;dxCoreC11.lib;dxLayoutControlC11.lib;cxEditorsC11.lib;cxDataC11.lib;dbrtl.lib;vcldb.lib;dbrtl.lib;bdertl.lib;adortl.lib;cxExtEditorsC11.lib;vclx.lib;cxPageControlC11.lib</AllPackageLibs>
    <ILINK_LibraryPath>$(BDS)\lib\release;$(BDS)\lib;$(BDS)\lib\obj;$(BDS)\lib\psdk;..\Common;..</ILINK_LibraryPath>
  </PropertyGroup>
  <ProjectExtensions>
    <Borland.Personality>CPlusPlusBuilder.Personality</Borland.Personality>
    <Borland.ProjectType>CppVCLApplication</Borland.ProjectType>
    <BorlandProject>
      <BorlandProject>
        <CPlusPlusBuilder.Personality>
        </CPlusPlusBuilder.Personality>
      </BorlandProject>
    </BorlandProject>
  </ProjectExtensions>
  <Import Project="$(MSBuildBinPath)\Borland.Cpp.Targets" />
  <ItemGroup>
    <ResFiles Include="PuzzleDemo.res">
    </ResFiles>
    <CppCompile Include="PuzzleDemo.cpp"> </CppCompile>
    <CppCompile Include="Puzzle.cpp">
      <Form>frmPuzzle</Form>
    </CppCompile>
    <CppCompile Include="..\Common\AboutDemoForm.cpp">
      <Form>formAboutDemo</Form>
    </CppCompile>
    <CppCompile Include="..\Common\BasicDemoMain.cpp">
      <Form>frmBasicDemoMain</Form>
    </CppCompile>
    <CppCompile Include="..\Common\DemoDM.cpp">
      <Form>dmDemo</Form>
    </CppCompile>
    <FormResources Include="Puzzle.dfm" />
    <FormResources Include="..\Common\AboutDemoForm.dfm" />
    <FormResources Include="..\Common\BasicDemoMain.dfm" />
    <FormResources Include="..\Common\DemoDM.dfm" />
  </ItemGroup>
</Project>